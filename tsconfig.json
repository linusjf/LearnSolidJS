{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmit": true,
    "outDir": "build",
    "rootDir": ".",
    "strict": true,
    "target": "ESNext",
    "types": [
      "vite/client"
    ]
  }
}
